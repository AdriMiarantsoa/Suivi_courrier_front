{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'connection_user',\n  data: function data() {\n    return {\n      nom_admin: '',\n      mot_de_passe: '',\n      erreurMessage: ''\n    };\n  },\n  methods: {\n    login: function login() {\n      var _this = this;\n      var formData = new FormData();\n      formData.append('nom_admin', this.nom_admin);\n      formData.append('mot_de_passe', this.mot_de_passe);\n      axios.post('http://localhost:8081/api/admin/login_admin', formData).then(function (response) {\n        console.log(response.data);\n        var token = response.data.token;\n        localStorage.setItem('authToken', token);\n        var redirectUrl = response.data.redirect;\n        _this.$router.push({\n          name: 'List of users not approved'\n        });\n      }).catch(function (error) {\n        console.error(error);\n        if (error.response) {\n          _this.erreurMessage = error.response.data;\n        } else {\n          _this.erreurMessage = 'Une erreur est survenue lors de la connexion.'; // Message générique\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","nom_admin","mot_de_passe","erreurMessage","methods","login","_this","formData","FormData","append","post","then","response","console","log","token","localStorage","setItem","redirectUrl","redirect","$router","push","catch","error"],"sources":["src/pages/LoginAdmin.vue"],"sourcesContent":["<template>\n    <div class=\"login-container\">\n      <div class=\"login-card\">\n        <!-- <img alt=\"User profile icon\" src=\"@/assets/ok.avif\" width=\"100\" height=\"100\" /> -->\n        <h2 style=\"color:#fff\">Login</h2>\n\n        <div v-if=\"erreurMessage\" class=\"error\">{{ erreurMessage }}</div>\n        <form @submit.prevent=\"login()\">\n          <div class=\"input-field\">\n            <input type=\"text\" id=\"nom\" v-model=\"nom_admin\" placeholder=\"Username\" />\n          </div>\n          <div class=\"input-field\">\n            <input type=\"password\" id=\"mdp\" v-model=\"mot_de_passe\" placeholder=\"Password\" />\n          </div>\n          <button type=\"submit\" class=\"login-btn\">LOGIN</button>\n        </form>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import axios from 'axios';\n  \n  export default {\n    name: 'connection_user',\n    data() {\n      return {\n        nom_admin: '',\n        mot_de_passe: '',\n\n        erreurMessage: ''\n      }\n    },\n    methods: {\n      login() {\n        const formData = new FormData();\n        formData.append('nom_admin', this.nom_admin);\n        formData.append('mot_de_passe', this.mot_de_passe);\n  \n        axios.post('http://localhost:8081/api/admin/login_admin', formData)\n          .then(response => {\n            console.log(response.data);\n            const token = response.data.token;\n            localStorage.setItem('authToken', token);\n            const redirectUrl = response.data.redirect;\n              this.$router.push({ name: 'List of users not approved' });\n          })\n          .catch(error => {\n            console.error(error);\n              if (error.response) {\n                this.erreurMessage = error.response.data;\n              } else {\n                  this.erreurMessage = 'Une erreur est survenue lors de la connexion.'; // Message générique\n              }\n          });\n      }\n    }\n  }\n  </script>\n  \n  <style>\n  body, html {\n    margin: 0;\n    padding: 0;\n    width: 100%;\n    height: 100%;\n    font-family: 'Arial', sans-serif;\n    background: linear-gradient(to bottom, rgba(240, 248, 255, 1), rgba(255, 255, 255, 1)); /* Light background gradient */\n    overflow: hidden;\n  }\n  \n  .login-page {\n    display: flex;\n    height: 100%;\n  }\n  \n  .text-animation {\n    width: 50%;\n    padding: 20px;\n    color: #001122; /* Dark text for contrast */\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: flex-start;\n    animation: fadeIn 2s ease-in-out;\n  }\n  \n  .text-animation h1 {\n    margin-bottom: 30px;\n    font-size: 2.5rem; /* Increased font size for emphasis */\n  }\n  \n  .text-animation p {\n    margin: 5px 0;\n    animation: slideIn 1.5s ease-in-out forwards;\n    font-size: 1.1rem; /* Slightly larger text for readability */\n  }\n  \n  .text-animation p:nth-child(2) {\n    animation-delay: 1s;\n  }\n  \n  .text-animation p:nth-child(3) {\n    animation-delay: 1.5s;\n  }\n  \n  .text-animation p:nth-child(4) {\n    animation-delay: 2s;\n  }\n  \n  .form-container {\n    width: 50%;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    padding: 30px;\n    margin-top: 10px;\n  }\n  \n  .form {\n    background: rgba(255, 255, 255, 0.95); /* Light background for form */\n    border-radius: 15px;\n    padding: 40px; /* Increased padding for spaciousness */\n    box-shadow: 0 4px 30px rgba(0, 0, 0, 0.1);\n    width: 100%;\n    max-width: 400px;\n    max-height: 80vh;\n    overflow-y: auto;\n  }\n  \n  .input-field {\n    margin-bottom: 20px;\n  }\n  \n  .input-field label {\n    display: block;\n    margin-bottom: 5px;\n    color: #001122; /* Consistent label color */\n    font-weight: bold; /* Bold labels for emphasis */\n  }\n  \n  .input-field input,\n  .input-field select {\n    width: 100%;\n    padding: 12px;\n    border: 2px solid #003366; /* Consistent border color */\n    border-radius: 25px;\n    outline: none;\n    font-size: 16px;\n    transition: border-color 0.3s;\n    background: rgba(240, 240, 240, 0.8); /* Light background for inputs */\n  }\n  \n  .input-field input:focus,\n  .input-field select:focus {\n    border-color: #005f8d; /* Highlight color on focus */\n  }\n  \n  .login-btn {\n    width: 100%;\n    padding: 12px;\n    border: none;\n    border-radius: 25px;\n    background: linear-gradient(to bottom, #0077b6, #005f8d); /* Gradient for button */\n    color: white;\n    font-size: 18px;\n    cursor: pointer;\n    transition: background 0.3s, transform 0.2s;\n  }\n  \n  .login-btn:hover {\n    background: linear-gradient(to bottom, #005f8d, #0077b6); /* Darker gradient on hover */\n  }\n  \n  .message {\n    text-align: center;\n    cursor: pointer;\n    color: #005f8d; /* Consistent message color */\n  }\n  \n  .error {\n    color: red;\n    margin-bottom: 10px;\n  }\n  \n  @keyframes fadeIn {\n    from {\n        opacity: 0;\n    }\n    to {\n        opacity: 1;\n    }\n  }\n  \n  @keyframes slideIn {\n    from {\n        transform: translateY(20px);\n        opacity: 0;\n    }\n    to {\n        transform: translateY(0);\n        opacity: 1;\n    }\n  }\n  \n  </style>\n  "],"mappings":";AAqBA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,SAAA;MACAC,YAAA;MAEAC,aAAA;IACA;EACA;EACAC,OAAA;IACAC,KAAA,WAAAA,MAAA;MAAA,IAAAC,KAAA;MACA,IAAAC,QAAA,OAAAC,QAAA;MACAD,QAAA,CAAAE,MAAA,mBAAAR,SAAA;MACAM,QAAA,CAAAE,MAAA,sBAAAP,YAAA;MAEAJ,KAAA,CAAAY,IAAA,gDAAAH,QAAA,EACAI,IAAA,WAAAC,QAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,QAAA,CAAAZ,IAAA;QACA,IAAAe,KAAA,GAAAH,QAAA,CAAAZ,IAAA,CAAAe,KAAA;QACAC,YAAA,CAAAC,OAAA,cAAAF,KAAA;QACA,IAAAG,WAAA,GAAAN,QAAA,CAAAZ,IAAA,CAAAmB,QAAA;QACAb,KAAA,CAAAc,OAAA,CAAAC,IAAA;UAAAtB,IAAA;QAAA;MACA,GACAuB,KAAA,WAAAC,KAAA;QACAV,OAAA,CAAAU,KAAA,CAAAA,KAAA;QACA,IAAAA,KAAA,CAAAX,QAAA;UACAN,KAAA,CAAAH,aAAA,GAAAoB,KAAA,CAAAX,QAAA,CAAAZ,IAAA;QACA;UACAM,KAAA,CAAAH,aAAA;QACA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}