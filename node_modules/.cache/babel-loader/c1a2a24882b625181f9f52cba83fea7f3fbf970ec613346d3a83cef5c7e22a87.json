rom 'axios';\n  \n  export default {\n    name: 'connection_user',\n    data() {\n      return {\n        nom_admin: '',\n        mot_de_passe: ''\n      }\n    },\n    methods: {\n      login() {\n        const formData = new FormData();\n        formData.append('nom_admin', this.nom_admin);\n        formData.append('mot_de_passe', this.mot_de_passe);\n  \n        axios.post('http://localhost:8081/api/admin/login_admin', formData)\n          .then(response => {\n            console.log(response.data);\n            const token = response.data.token;\n            localStorage.setItem('authToken', token);\n            const redirectUrl = response.data.redirect;\n              this.$router.push({ name: 'List of users not approved' });\n          })\n          .catch(error => {\n            console.error(error);\n          });\n      }\n    }\n  }\n  </script>\n  \n  <style>\n  * {\n    box-sizing: border-box;\n    margin: 0;\n    padding: 0;\n  }\n  \n  body {\n    margin: 0;\n    padding: 0;\n  }\n  \n  .login-container {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 100vh;\n    width: 100vw;\n    background-color: #333;\n  }\n  \n  .login-card {\n    background-color: rgba(0, 0, 0, 0.7);\n    padding: 20px;\n    border-radius: 10px;\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);\n    text-align: center;\n    width: 300px;\n  }\n  \n  .login-card img {\n    width: 80px;\n    height: 80px;\n    border-radius: 50%;\n  }\n  \n  .input-field {\n    position: relative;\n  }\n  \n  .input-field input[type=\"text\"],\n  .input-field input[type=\"password\"],\n  .input-field select {\n    width: calc(100% - 30px);\n    padding: 10px 10px 10px 30px;\n    margin: 10px 0;\n    border: none;\n    border-radius: 5px;\n    background-color: #444;\n    color: #fff;\n  }\n  \n  .input-field select {\n    appearance: none; /* Removes default arrow */\n    background-color: #444; /* Background color */\n    color: #fff; /* Text color */\n    background-image: url('data:image/svg+xml;utf8,<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"10\" height=\"10\" viewBox=\"0 0 10 10\"><polygon points=\"0,0 5,5 10,0\" fill=\"%23fff\"/></svg>'); /* Custom arrow */\n    background-repeat: no-repeat;\n    background-position: right 10px center; /* Position the arrow */\n  }\n  \n  .remember-me a {\n    color: #999;\n    text-decoration: none;\n    font-size: 12px;\n    float: right;\n  }\n  \n  .remember-me a:hover {\n    text-decoration: underline;\n  }\n  \n  .login-btn {\n    width: 100%;\n    padding: 10px;\n    border: none;\n    border-radius: 5px;\n    background-color: #555; \n    color: #fff; \n    font-size: 16px;\n    cursor: pointer;\n  }\n  \n  .login-btn:hover {\n    background-color: #666; \n  }\n  \n  .forgot-password {\n    font-size: 14px;\n    color: #fff; \n    text-align: center;\n    margin-top: 20px;\n  }\n  \n  .toggle-mode {\n    font-size: 14px;\n    color: #666; \n    cursor: pointer;\n    margin-top: 20px;\n  }\n  \n  .toggle-mode:hover {\n    text-decoration: underline;\n  }\n  </style>\n  "],"mappings":";AAmBA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,SAAA;MACAC,YAAA;IACA;EACA;EACAC,OAAA;IACAC,KAAA,WAAAA,MAAA;MAAA,IAAAC,KAAA;MACA,IAAAC,QAAA,OAAAC,QAAA;MACAD,QAAA,CAAAE,MAAA,mBAAAP,SAAA;MACAK,QAAA,CAAAE,MAAA,sBAAAN,YAAA;MAEAJ,KAAA,CAAAW,IAAA,gDAAAH,QAAA,EACAI,IAAA,WAAAC,QAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,QAAA,CAAAX,IAAA;QACA,IAAAc,KAAA,GAAAH,QAAA,CAAAX,IAAA,CAAAc,KAAA;QACAC,YAAA,CAAAC,OAAA,cAAAF,KAAA;QACA,IAAAG,WAAA,GAAAN,QAAA,CAAAX,IAAA,CAAAkB,QAAA;QACAb,KAAA,CAAAc,OAAA,CAAAC,IAAA;UAAArB,IAAA;QAAA;MACA,GACAsB,KAAA,WAAAC,KAAA;QACAV,OAAA,CAAAU,KAAA,CAAAA,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}